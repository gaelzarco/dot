" Global defaults
" set termguicolors
set nocompatible
set number
set relativenumber
set wildmenu
set nowrap
set showmatch
set tabstop=4
set softtabstop=4
set shiftwidth=4
set colorcolumn=80
set scrolloff=8
set sidescrolloff=8
set list
set listchars=tab:→\ ,trail:·,nbsp:␣
set expandtab
set noswapfile
set pumheight=10
set fillchars=eob:\
set textwidth=80
set clipboard=unnamedplus

hi Normal guibg=NONE ctermbg=NONE
hi NormalFloat guibg=NONE ctermbg=NONE
hi statusline guibg=NONE
hi LineNr guibg=NONE
hi CursorLine cterm=NONE gui=NONE guibg=#1a1a1a
hi CursorLineNr guibg=NONE
hi SignColumn guibg=NONE
highlight ColorColumn guibg=#111111 ctermbg=233
